{{#selectedProject}}
    <div class="project">
        <h1>{{name}}</h1>
        <a href="../{{path}}" target="_blank"><img class="big-img" src="/{{path}}/screenshot.png"></a>
        <h1 class="description">{{description}}</h1>
        <form target="_blank" action="../{{path}}">
            <button type="submit">Launch</button>
        </form>
    </div>
{{/selectedProject}}